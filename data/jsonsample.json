{
   "name":"mnist_siamese",
   "input":"data",
   "input_shape":{"dim":[10000, 1, 28, 28]},
   "layer":[{
      "name":"conv1",
      "type":"Convolution",
      "bottom":"data",
      "top":"conv1",
      "param":[{
         "lr_mult":1
      }, {
         "lr_mult":2
      }],
      "convolution_param":{
         "num_output":20,
         "kernel_size":5,
         "stride":1
      }
   },
   {
      "name":"pool1",
      "type":"Pooling",
      "bottom":"conv1",
      "top":"pool1",
      "pooling_param":{
         "pool":"MAX",
         "kernel_size":2,
         "stride":2
      }
   },
   {
      "name":"ip1",
      "type":"InnerProduct",
      "bottom":"pool2",
      "top":"ip1",
      "param":[
         {
            "lr_mult":1
         },
         {
            "lr_mult":2
         }
      ],
      "inner_product_param":{
         "num_output":500
      }
   },
      {
         "name": "relu1",
         "type": "ReLU",
         "bottom": "ip1",
         "top": "ip1"
      }
   ]
}